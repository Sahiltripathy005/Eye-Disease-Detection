    "import cv2\n",
    "import os\n",
    "import numpy as np\n",
    "from tqdm import tqdm\n",
    "\n",
    "def calculate_iou(segmented_image, ground_truth_image):\n",
    "    \"\"\"\n",
    "    Calculates the Intersection over Union (IoU) score between two images.\n",
    "\n",
    "    Args:\n",
    "        segmented_image (numpy.ndarray): The segmented image.\n",
    "        ground_truth_image (numpy.ndarray): The ground truth image.\n",
    "\n",
    "    Returns:\n",
    "        float: The IoU score.\n",
    "    \"\"\"\n",
    "\n",
    "    # Resize the ground truth image to match the segmented image\n",
    "    ground_truth_image_resized = cv2.resize(ground_truth_image, (segmented_image.shape[1], segmented_image.shape[0]))\n",
    "\n",
    "    # Ensure both images are binary (0 and 255) or normalize them to 0 and 1\n",
    "    segmented_image = cv2.threshold(segmented_image, 128, 255, cv2.THRESH_BINARY)[1] // 255\n",
    "    ground_truth_image_resized = cv2.threshold(ground_truth_image_resized, 128, 255, cv2.THRESH_BINARY)[1] // 255\n",
    "\n",
    "    # Intersection (True Positives)\n",
    "    intersection = np.logical_and(segmented_image, ground_truth_image_resized)\n",
    "\n",
    "    # Union (True Positives + False Positives + False Negatives)\n",
    "    union = np.logical_or(segmented_image, ground_truth_image_resized)\n",
    "\n",
    "    # Prevent division by zero by checking if the union is non-zero\n",
    "    if np.sum(union) == 0:\n",
    "        return 1.0 if np.sum(intersection) == 0 else 0.0  # If both are empty, IoU is 1.0, otherwise 0.0\n",
    "\n",
    "    # IoU calculation\n",
    "    iou_score = np.sum(intersection) / np.sum(union)\n",
    "\n",
    "    return iou_score\n",
    "\n",
    "\n",
    "def calculate_iou_for_dataset(preprocessed_dataset_path, ground_truth_path):\n",
    "    \"\"\"\n",
    "    Calculates the average IoU score for a dataset.\n",
    "\n",
    "    Args:\n",
    "        preprocessed_dataset_path (str): Path to the preprocessed dataset directory.\n",
    "        ground_truth_path (str): Path to the ground truth dataset directory.\n",
    "\n",
    "    Returns:\n",
    "        float: The average IoU score for the dataset.\n",
    "    \"\"\"\n",
    "\n",
    "    iou_scores = []\n",
    "\n",
    "    # Loop through each subfolder (disease types)\n",
    "    for subfolder in os.listdir(preprocessed_dataset_path):\n",
    "        preprocessed_subfolder = os.path.join(preprocessed_dataset_path, subfolder)\n",
    "        ground_truth_subfolder = os.path.join(ground_truth_path, subfolder)\n",
    "\n",
    "        if os.path.isdir(preprocessed_subfolder) and os.path.isdir(ground_truth_subfolder):\n",
    "            for img_file in tqdm(os.listdir(preprocessed_subfolder)):\n",
    "                preprocessed_img_path = os.path.join(preprocessed_subfolder, img_file)\n",
    "                ground_truth_img_path = os.path.join(ground_truth_subfolder, img_file)\n",
    "\n",
    "                if os.path.exists(ground_truth_img_path) and img_file.lower().endswith(('.png', '.jpg', '.jpeg')):\n",
    "                    # Load both segmented and ground truth images\n",
    "                    segmented_image = cv2.imread(preprocessed_img_path, cv2.IMREAD_GRAYSCALE)\n",
    "                    ground_truth_image = cv2.imread(ground_truth_img_path, cv2.IMREAD_GRAYSCALE)\n",
    "\n",
    "                    if segmented_image is not None and ground_truth_image is not None:\n",
    "                        # Calculate IoU and append to the list\n",
    "                        iou_score = calculate_iou(segmented_image, ground_truth_image)\n",
    "                        iou_scores.append(iou_score)\n",
    "\n",
    "    return iou_scores\n",
    "\n",
    "\n",
    "# Specify paths for your dataset\n",
    "preprocessed_dataset_path = \"../data/processed\"\n",
    "ground_truth_path = \"../data/ground_truth_dataset\"\n",
    "\n",
    "# Calculate IoU scores for the dataset\n",
    "iou_scores = calculate_iou_for_dataset(preprocessed_dataset_path, ground_truth_path)\n",
    "\n",
    "# Calculate and print the average IoU score for all images\n",
    "average_iou = np.mean(iou_scores)\n",
    "print(f'Average IoU score for the dataset: {average_iou:.4f}')"
   ]
