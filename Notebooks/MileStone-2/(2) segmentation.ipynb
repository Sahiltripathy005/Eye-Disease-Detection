    "#Function for applying segmentation on all the images\n",
    "import cv2\n",
    "import os\n",
    "import numpy as np\n",
    "from tqdm import tqdm\n",
    "\n",
    "# Path for the preprocessed dataset\n",
    "preprocessed_dataset_path = \"D:\MEDISCAN\data\processed\"\n",
    "\n",
    "# Function for image segmentation using thresholding\n",
    "def segment_image(image):\n",
    "    # Convert to grayscale\n",
    "    gray_image = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)\n",
    "    \n",
    "    # Apply a binary threshold to create a segmentation mask\n",
    "    _, segmented_image = cv2.threshold(gray_image, 128, 255, cv2.THRESH_BINARY)\n",
    "    \n",
    "    return segmented_image\n",
    "\n",
    "# Apply segmentation to all images in the preprocessed dataset\n",
    "def segment_preprocessed_images(dataset_path):\n",
    "    # Loop through each subfolder (representing disease types)\n",
    "    for subfolder in os.listdir(dataset_path):\n",
    "        subfolder_path = os.path.join(dataset_path, subfolder)\n",
    "        \n",
    "        # Process each image in the subfolder\n",
    "        if os.path.isdir(subfolder_path):\n",
    "            for img_file in tqdm(os.listdir(subfolder_path)):\n",
    "                img_path = os.path.join(subfolder_path, img_file)\n",
    "                \n",
    "                if img_file.lower().endswith(('.png', '.jpg', '.jpeg')):\n",
    "                    image = cv2.imread(img_path)\n",
    "                    if image is not None:\n",
    "                        # Apply segmentation\n",
    "                        segmented_image = segment_image(image)\n",
    "                        \n",
    "                        # Save the segmented image, replacing the original\n",
    "                        save_image_path = os.path.join(subfolder_path, img_file)\n",
    "                        cv2.imwrite(save_image_path, segmented_image)\n",
    "\n",
    "# Call the function to apply segmentation to all images\n",
    "segment_preprocessed_images(preprocessed_dataset_path)"
   ]
  },
