    "#The function that generates binary masks that highlight the regions of interest (e.g., diseased areas) within medical images. \n",
    "#These masks can be used for training and evaluation of image segmentation models.\n",
    "\n",
    "import cv2\n",
    "import os\n",
    "import matplotlib.pyplot as plt\n",
    "from tqdm import tqdm\n",
    "\n",
    "def create_ground_truth_masks(raw_dataset_path, ground_truth_path):\n",
    "    \"\"\"\n",
    "    Creates ground truth masks for medical images.\n",
    "\n",
    "    Args:\n",
    "        raw_dataset_path (str): Path to the raw dataset directory.\n",
    "        ground_truth_path (str): Path to the directory where ground truth masks will be saved.\n",
    "    \"\"\"\n",
    "\n",
    "    # Loop through each subfolder (representing disease types)\n",
    "    for subfolder in os.listdir(raw_dataset_path):\n",
    "        raw_subfolder_path = os.path.join(raw_dataset_path, subfolder)\n",
    "        ground_truth_subfolder_path = os.path.join(ground_truth_path, subfolder)\n",
    "\n",
    "        if not os.path.exists(ground_truth_subfolder_path):\n",
    "            os.makedirs(ground_truth_subfolder_path)\n",
    "\n",
    "        # Process each image in the subfolder\n",
    "        if os.path.isdir(raw_subfolder_path):\n",
    "            for img_file in tqdm(os.listdir(raw_subfolder_path)):\n",
    "                img_path = os.path.join(raw_subfolder_path, img_file)\n",
    "\n",
    "                if img_file.lower().endswith(('.png', '.jpg', '.jpeg')):\n",
    "                    image = cv2.imread(img_path)\n",
    "\n",
    "                    if image is not None:\n",
    "                        # Convert to grayscale\n",
    "                        gray_image = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)\n",
    "\n",
    "                        # Apply binary thresholding (adjust threshold as needed)\n",
    "                        _, mask = cv2.threshold(gray_image, 128, 255, cv2.THRESH_BINARY)\n",
    "\n",
    "                        # Save the mask as the ground truth\n",
    "                        save_mask_path = os.path.join(ground_truth_subfolder_path, img_file)\n",
    "                        cv2.imwrite(save_mask_path, mask)\n",
    "\n",
    "# Specify paths for your dataset\n",
    "raw_dataset_path = \"../data/raw\"\n",
    "ground_truth_path = \"../data/ground_truth_dataset\"\n",
    "\n",
    "# Create ground truth masks\n",
